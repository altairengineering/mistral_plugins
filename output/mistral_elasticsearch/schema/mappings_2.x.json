{
  "template" : "mistral-*",
  "mappings": {
    "_default_": {
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "strict_date_optional_time||epoch_millis||epoch_second"
        },
        "job" : {
          "properties": {
            "host" : {
            "type": "string",
            "index": "not_analyzed"
            },
            "job-id" : {
            "type": "string",
            "index": "not_analyzed"
            }
            },
            "job-group-id" : {
            "type": "string",
            "index": "not_analyzed"
            }
          }
        },
        "process" : {
          "properties": {
            "command" : {
            "type": "string",
            "index": "not_analyzed"
            },
            "file" : {
            "type": "string",
            "index": "not_analyzed"
            }
          }
        }
      }
    }
  }
}
